{
  "name": "factly-covid19",
  "version": "1.0.0",
  "license": "ISC",
  "description": "Covide 19 factly website",
  "repository": {
    "type": "git",
    "url": "https://github.com/factly/covid-19-english"
  },
  "main": "gatsby-node.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "gatsby build",
    "start": "gatsby develop",
    "dev-m": "gatsby develop -H 0.0.0.0",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "scsslint": "sass-lint-auto-fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && npm run format && npm run scsslint"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "prettier": "^2.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@bumped-inc/gatsby-plugin-optional-chaining": "^1.0.0",
    "@fs/gatsby-plugin-drive": "^0.2.1",
    "axios": "^0.19.2",
    "date-fns": "^2.11.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.18.7",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-source-custom-api": "^2.1.4",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-source-wordpress": "file:plugins/gatsby-source-wordpress",
    "gatsby-transformer-sharp": "^2.3.7",
    "gtoken": "^5.0.3",
    "install": "^0.13.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-images": "^1.1.0",
    "react-infinite-scroller": "^1.2.4",
    "react-photo-gallery": "^8.0.0",
    "sass-lint": "^1.13.1"
  }
}
